{
  "name": "@nexical/generator",
  "version": "0.1.0",
  "type": "module",
  "description": "Nexical Generator CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "arc": "./dist/bin.js"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc -b tsconfig.build.json && chmod +x dist/bin.js",
    "dev": "tsc -w",
    "start": "node dist/bin.js",
    "test:unit": "vitest run -c vitest.unit.config.ts",
    "test:unit:watch": "vitest -c vitest.unit.config.ts",
    "test:unit:coverage": "vitest run -c vitest.unit.config.ts --coverage"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^13.0.0",
    "inquirer": "^9.2.14",
    "ora": "^8.0.1",
    "prettier": "3.8.1",
    "ts-morph": "^21.0.1",
    "yaml": "^2.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3"
  }
}
