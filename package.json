{
  "name": "app-core",
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "modules/*",
    "packages/*"
  ],
  "bin": {
    "prompt": "./scripts/prompt.ts",
    "reskill": "./scripts/reskill.ts"
  },
  "scripts": {
    "astro": "astro",
    "build": "npm run setup && astro build",
    "core:check": "npx tsx scripts/sentinel.ts check",
    "docgap": "docgap",
    "core:accept": "npx tsx scripts/sentinel.ts accept",
    "db:destroy": "docker compose -f compose.db.yml down --volumes",
    "db:down": "docker compose -f compose.db.yml down",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:up": "docker compose -f compose.db.yml up -d",
    "db:validate": "npx prisma validate",
    "dev": "npm run setup && npm run db:up && astro dev",
    "docker:destroy": "docker compose down --volumes --remove-orphans",
    "docker:down": "docker compose down --remove-orphans",
    "docker:up": "docker compose up",
    "docker:start": "docker compose up -d",
    "gen:sdk": "npx tsx scripts/generate-sdk.ts",
    "gen:agent": "npx tsx scripts/generate-agent-registry.ts",
    "dev:agent": "npm run gen:agent && npm run dev --workspace=@app-core/agent",
    "postinstall": "npm run build --workspace=@nexical/generator",
    "prepare": "husky",
    "preview": "npm run db:up && astro preview",
    "prompt": "npx tsx scripts/prompt.ts",
    "reskill": "npx tsx scripts/reskill.ts",
    "setup:db": "npx tsx scripts/generate-prisma.ts && npm run db:generate && npm run db:push && npm run db:seed",
    "setup:env": "npx tsx scripts/generate-env.ts",
    "setup:theme": "npx tsx scripts/generate-theme.ts",
    "setup:version": "npx tsx scripts/generate-version.ts",
    "setup": "npm run setup:env && npm run setup:db && npm run setup:version && npm run setup:theme && npm run gen:sdk",
    "test:agents": "vitest run -c vitest.agent.config.ts",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:integration": "npx vitest run -c vitest.integration.config.ts",
    "test:unit": "npx vitest run -c vitest.unit.config.ts",
    "test": "npm run test:unit && npm run test:integration && npm run test:agents && npm run test:e2e"
  },
  "overrides": {
    "vite": "^6.4.1",
    "@auth/core": "^0.41.1",
    "hono": "^4.11.4",
    "diff": "^8.0.3"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/node": "^9.5.1",
    "@astrojs/react": "^4.4.2",
    "@hookform/resolvers": "^5.2.2",
    "@nexical/sdk": "*",
    "@panva/hkdf": "^1.2.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.3.0",
    "@prisma/config": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.2",
    "@react-email/render": "^2.0.0",
    "@scalar/api-reference-react": "^0.8.15",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "defu": "^6.1.4",
    "framer-motion": "^12.23.26",
    "globby": "^16.1.0",
    "i18next": "^25.7.3",
    "i18next-http-backend": "^3.0.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "nodemailer": "^7.0.0",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^16.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul": "^1.1.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@nexical/docgap": "^0.2.3",
    "@nexical/generator": "*",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/nodemailer": "^7.0.5",
    "@types/nunjucks": "^3.2.6",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "minimist": "^1.2.8",
    "nunjucks": "^3.2.4",
    "prisma": "7.3.0",
    "repomix": "^1.11.1",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}